     btnLimpiar.setEnabled(false);
        
        
     txtFecha.setText("");
     txtDia.setText("");
     txtMes.setText("");
     txtAño.setText("");
     txtEstacion.setText("");
     txtFecha.requestFocus( );        // TODO add your handling code here:
    }//GEN-LAST:event_btnLimpiarActionPerformed

