    private void btnejecutarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnejecutarActionPerformed
String texto=txtFecha.getText();

 String primera=texto.substring(0,2);
 txtDia.setText(primera);

 String segunda=texto.substring(3,5);
 txtMes.setText(segunda);
 
 String tercera=texto.substring(6,10);
 txtAño.setText(tercera);

 int mes,dia;
mes=parseInt(segunda);
if mes 
 
 


    }//GEN-LAST:event_btnejecutarActionPerformed

    
